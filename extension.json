{
	"name": "WikiPoints",
	"author": "Tali64",
	"descriptionmsg": "wikipoints-description",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"version": "1.0.0",
	"type": "specialpage",
	"license-name": "GPL-3.0-or-later",
	"manifest_version": 2,
	"AutoloadClasses": {
		"MediaWiki\\Extension\\WikiPoints\\SpecialWikiPoints": "includes/SpecialWikiPoints.php",
		"MediaWiki\\Extension\\WikiPoints\\SpecialMostWikiPoints": "includes/SpecialMostWikiPoints.php"
	},
	"MessagesDirs": {
		"WikiPoints": [
			"i18n"
		]
	},
	"SpecialPages": {
		"WikiPoints": {
			"class": "MediaWiki\\Extension\\WikiPoints\\SpecialWikiPoints",
			"services": [
				"ConnectionProvider",
				"UserFactory"
			]
		},
		"MostWikiPoints": {
			"class": "MediaWiki\\Extension\\WikiPoints\\SpecialMostWikiPoints",
			"services": [
				"ConnectionProvider",
				"LinkRenderer",
				"SpecialPageFactory"
			]
		}
	}
}
